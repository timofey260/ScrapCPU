COMM FILLS DISPLAY WITH SYBMOT IN NUM REGISTER
MARK FILL
	COPY 0 TOUCHFUSE
	SKIP 2
	COPY 0 CYCLEVAL
	COPY NUM LEN
	MARK CYCLEFILL
		DIVI CYCLEVAL 9 X
		FLOR X X
		DVRI CYCLEVAL 9 Y
		COPY LEN RAX
		CALL SET

		ADDI CYCLEVAL 1 CYCLEVAL
		TEST CYCLEVAL < 81 CYCLEFILL
	COPY LEN A1
	RETI

